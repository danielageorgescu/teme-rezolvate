package com.itfactory;

public class Main {
    /**
     * Sa se afiseze numerele de la 0 la 20 pe aceeasi linie utilizand o bucla for
     *
     * @param args
     */
    public static void main(String[] args) {
        {
            int x;
            for (x = 0; x <= 20; x++) {
                System.out.print( x + "  ");
            }
        }
    }
}
